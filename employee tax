empname=input('name:')
bs=int(input('basic salary:'))
da=int(input('daily alloance:'))
ta=int(input('traveling allowance:'))
hra=int(input('house rental allowance:' ))
netsalary=int(input('net salary:'))
netsalary=bs+da+ta+hra
if netsalary>100000:
    tax=netsalary*10/100
elif netsalary>50000:
    tax=netsalary*7/100
elif netsalary>40000:
    tax=netsalary*4/100
elif netsalary>20000:
    tax=netsalary*2/100
else:
    tax=0
print('tax',tax)
