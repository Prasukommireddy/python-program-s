def year(n):
    if n>=18:
        print('yes')
    else:
        print('no')
n=int(input())
year(n)

*******
def values(n):
    if n>=18:
        return True
    else:
        return False
n=int(input())
if values(n):
    print('yes')
else:
    print('no')
    
    **********
   n=int(input())
if n>=18:
    print('yes')
else:
    print('no')
